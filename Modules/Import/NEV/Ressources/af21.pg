flivraison_type text,
flivraison_id text,
                CONSTRAINT af21_pkey PRIMARY KEY (fdate,fchrono)
            )
            WITH (
                OIDS = FALSE
            )
            TABLESPACE pg_default;

        execute 'COPY af21(
            fdate,
fchrono,
findex,
fetatfacture,
ftypefacture,
fnovendeur,
fnointerne,
fnoorg,
ffactorg,
fpiece_date,
fpiece_chrono,
fdelivrance,
fnomedecin,
fbasetaux,
fpartorg,
fpartclient,
ftauxrembt,
furgence,
fcopie,
fforfait,
fremise,
fdateordonnance,
