#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = ibstr.dll ibpha.dll ibfichier.dll
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

ibstr.dll: ibstr\ibstr.dpr
  $(DCC)

ibpha.dll: ibpha\ibpha.dpr
  $(DCC)

ibfichier.dll: ibfichier\ibfichier.dpr
  $(DCC)


